      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:c="http://java.sun.com/jstl/core"
      xmlns:e="http://www.enverio.com/jsf/samples">
<body>

<ui:composition>
<fieldset id="#{id}" class="#{class}">
	<c:if test="#{not empty title}">				
		<e:foo>
			Goo
			#{title}
			Moo
		</e:foo>
	</c:if>
	<ui:insert/>
</fieldset>
</ui:composition>

</body>
</html>